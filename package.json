{
  "name": "urbanisierung.dev",
  "version": "1.0.0",
  "description": "urbanisierung.dev",
  "main": "index.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:int": "ng build --aot --configuration=\"integration\"",
    "build:prod": "ng build --aot --configuration=\"production\"",
    "test": "ng test",
    "ng-lint": "ng lint",
    "lint": "eslint -c .eslintrc.js --fix \"src/**/**.ts\"",
    "e2e": "ng e2e",
    "build:stats": "ng build --prod --stats-json",
    "build:all": "npm run build:stats && npm run analyze"
  },
  "repository": "github:urbanisierung/urbanisierung",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,markdown}": [
      "prettier --write",
      "git add"
    ],
    "*.{json,css,html,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,jsx}": [
      "prettier --write",
      "eslint -c .eslintrc.js --fix",
      "git add"
    ]
  },
  "author": "Adam Urban",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "~10.2.4",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@angular/service-worker": "~12.0.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "bulma": "^0.9.2",
    "rxjs": "~6.6.7",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@angular/language-service": "~12.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "codelyzer": "^6.0.0",
    "critical": "^3.0.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.1",
    "typescript": "~4.2.4",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
